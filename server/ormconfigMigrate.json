{
	"type": "postgres",
	"synchronize": false,
	"logging": false,
    "migrationsRun": true,
    "extra": {
        "ssl": {
            "rejectedUnauthorized": false
        }
    },
	"entities": ["dist/entity/**/*.js"],
	"migrations": ["dist/migration/**/*.js"],
	"subscribers": ["dist/subscriber/**/*.js"],
	"cli": {
		"entitiesDir": "src/entity",
		"migrationsDir": "src/migration",
		"subscribersDir": "src/subscriber"
	}
}