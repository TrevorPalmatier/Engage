{
   "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "noImplicitAny": true,
      "outDir": "dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
         "*": [
            "node_modules/*"
         ]
      },
      "types": ["node"]
   },
   "exclude":[
      "node_modules",
      "./node_modules",
      "./node_modules/*"
   ],
   "include": [
      "src/**/*",
      "src/migration*"
   ]
}